{
  "dns": {
    "fakeip": {
      "enabled": false,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "rules": [
      {
        "outbound": "any",
        "server": "direct"
      },
      {
        "rule_set": [
          "geosite-private",
          "geosite-cn"
        ],
        "server": "direct"
      },
      {
        "disable_cache": true,
        "rule_set": [
          "geosite-category-ads-all",
          "geosite-cryptominers",
          "geosite-malware",
          "geosite-phishing",
          "geoip-malware",
          "geoip-phishing"
        ],
        "server": "block"
      }
    ],
    "servers": [
      {
        "address": "https://8.8.4.4/dns-query",
        "tag": "remote"
      },
      {
        "address": "tls://223.5.5.5",
        "detour": "direct",
        "tag": "direct"
      },
      {
        "address": "rcode://refused",
        "tag": "block"
      }
    ],
    "strategy": "prefer_ipv6"
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "xd",
      "external_ui_download_detour": "direct",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/gh-pages.zip"
    },
    "debug": {
      "memory_limit": "200 MB",
      "oom_killer": true
    }
  },
  "inbounds": [
    {
      "domain_strategy": "prefer_ipv6",
      "listen": "::",
      "listen_port": 7890,
      "set_system_proxy": true,
      "sniff": true,
      "sniff_override_destination": true,
      "tcp_fast_open": true,
      "tcp_multi_path": true,
      "type": "mixed",
      "udp_fragment": true
    }
  ],
  "log": {
    "level": "fatal",
    "timestamp": true
  },
  "ntp": {
    "detour": "direct",
    "enabled": true,
    "server": "pool.ntp.org"
  },
  "outbounds": [
    {
      "interrupt_exist_connections": true,
      "outbounds": [
        "auto",
        "manual",
        "direct",
        "block"
      ],
      "tag": "default",
      "type": "selector"
    },
    {
      "interval": "30m",
      "outbounds": [],
      "tag": "auto",
      "type": "urltest",
      "url": "https://www.youtube.com/generate_204"
    },
    {
      "interrupt_exist_connections": true,
      "outbounds": [],
      "tag": "manual",
      "type": "selector"
    },
    {
      "tag": "direct",
      "type": "direct"
    },
    {
      "tag": "block",
      "type": "block"
    },
    {
      "tag": "dns",
      "type": "dns"
    }
  ],
  "route": {
